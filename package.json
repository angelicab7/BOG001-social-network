{
  "name": "social-network",
  "version": "1.0.0",
  "description": "Social Network @ Laboratoria",
  "main": "src/main.js",
  "keywords": [
    "javascript",
    "firebase",
    "jest"
  ],
  "license": "MIT",
  "scripts": {
    "htmlhint": "htmlhint dist/*.html ",
    "eslint": "eslint --ext .js src/ test/",
    "eslint:fix": "eslint --ext .js src/ test/ --fix",
    "stylelint": "stylelint --aei src/**/*.css",
    "pretest": "npm run htmlhint && npm run eslint && npm run stylelint",
    "test": "jest --coverage",
    "jest": "jest --coverage",
    "start": "webpack-dev-server",
    "build": "webpack --mode production",
    "deploy": "rm -rf dist && npm run build && gh-pages -d dist"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.3.0",
    "copy-webpack-plugin": "^6.0.4",
    "css-loader": "^4.2.2",
    "eslint": "^7.7.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "file-loader": "^6.1.0",
    "html-loader": "^1.3.0",
    "html-webpack-plugin": "^4.4.1",
    "htmlhint": "^0.14.1",
    "jest": "^26.4.2",
    "node-sass": "^4.14.1",
    "regenerator-runtime": "^0.13.7",
    "sass-loader": "^10.0.1",
    "serve": "^11.3.2",
    "style-loader": "^1.2.1",
    "stylelint": "^13.7.0",
    "stylelint-config-recommended": "^3.0.0",
    "svg-inline-loader": "^0.8.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "firebase": "^7.19.1",
    "sweetalert2": "^9.17.1"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|html)$": "<rootDir>/fileMock.js",
      "\\.(css|less|scss|sass)$": "<rootDir>/styleMock.js"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "coverageReporters": [
      "json",
      "text",
      "lcov",
      "clover",
      "html"
    ]
  }
}
